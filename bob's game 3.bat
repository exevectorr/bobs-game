:boss_fight
cls
color 4f
title BOSS FIGHT - Guardian Sunster
setlocal enabledelayedexpansion

:: Boss stats
set /a sunster_hp=500

:: Player stats fortsätter från tidigare (HP sparas), energi startar på 200
if not defined player_energy set player_energy=200
if not defined player_hp set player_hp=100

set dodged=0

echo ===============================
echo         BOSS FIGHT!!!
echo     GUARDIAN SUNSTER APPEARS
echo ===============================
echo.
echo The mighty Guardian Sunster stands in your way!
echo It radiates solar heat and protects the Noomsters' secrets...
pause

:boss_combat
cls
echo Your HP: !player_hp!      Energy: !player_energy!
echo Guardian Sunster HP: !sunster_hp!
echo.
echo Choose your action:

if "!weapon!"=="sword" (
    echo 1. Sword Slash (15-25 damage)
    echo 2. Sword Slam (60%% hit chance, 30-50 damage)
    echo 3. Dodge (Costs 50 Energy, 50%% chance to avoid next attack)
) else (
    echo 1. Earth Bending (30-45 damage)
    echo 2. Ground Slam (60%% hit chance, 40-60 damage)
    echo 3. Dodge (Costs 50 Energy, 50%% chance to avoid next attack)
)

set /p move=Choose (1/2/3): 

if "!weapon!"=="sword" (
    if "!move!"=="1" (
        set /a dmg=15 + !random! %% 11
        echo You slash your sword and hit Guardian Sunster for !dmg! damage!
        set /a sunster_hp-=dmg
        set dodged=0
    ) else if "!move!"=="2" (
        set /a chance=!random! %% 100
        if !chance! LSS 60 (
            set /a dmg=30 + !random! %% 21
            echo Sword Slam succeeded! You dealt !dmg! damage!
            set /a sunster_hp-=dmg
        ) else (
            echo Sword Slam missed!
        )
        set dodged=0
    ) else if "!move!"=="3" (
        if !player_energy! GEQ 50 (
            set /a player_energy-=50
            set /a chance=!random! %% 100
            if !chance! LSS 50 (
                echo You successfully dodged the next attack!
                set dodged=1
            ) else (
                echo Dodge failed! You will take the next attack as usual.
                set dodged=0
            )
        ) else (
            echo Not enough energy to dodge!
            set dodged=0
        )
    ) else (
        echo Invalid choice!
        timeout /t 1 >nul
        goto boss_combat
    )
) else (
    if "!move!"=="1" (
        set /a dmg=30 + !random! %% 16
        echo You unleash powerful Earth Bending and hit for !dmg! damage!
        set /a sunster_hp-=dmg
        set dodged=0
    ) else if "!move!"=="2" (
        set /a chance=!random! %% 100
        if !chance! LSS 60 (
            set /a dmg=40 + !random! %% 21
            echo Ground Slam succeeded! You dealt !dmg! damage!
            set /a sunster_hp-=dmg
        ) else (
            echo Ground Slam missed!
        )
        set dodged=0
    ) else if "!move!"=="3" (
        if !player_energy! GEQ 50 (
            set /a player_energy-=50
            set /a chance=!random! %% 100
            if !chance! LSS 50 (
                echo You successfully dodged the next attack!
                set dodged=1
            ) else (
                echo Dodge failed! You will take the next attack as usual.
                set dodged=0
            )
        ) else (
            echo Not enough energy to dodge!
            set dodged=0
        )
    ) else (
        echo Invalid choice!
        timeout /t 1 >nul
        goto boss_combat
    )
)

if !sunster_hp! LEQ 0 (
    goto boss_victory
)

:: Boss attacks only if player didn't successfully dodge
if !dodged! EQU 1 (
    echo Guardian Sunster's attack was dodged!
) else (
    set /a enemy_dmg=15 + !random! %% 11
    echo Guardian Sunster attacks you for !enemy_dmg! damage!
    set /a player_hp-=enemy_dmg
)

if !player_hp! LEQ 0 (
    goto boss_defeat
)

timeout /t 2 >nul
goto boss_combat

